<%- include("partials/header") -%>
<section class = "formsec">
  <form action="/register" method="post" class= "form-signin text-center" enctype= "multipart/form-data">

    <label class = "derlan"><%= tiller %></label>
    <br /><br />
    <p class ="title"><%= typeof msg != 'undefined' ? msg : '' %></p>
    <input class = "form-control sumber btn" id= "file" type="file" name="myImage">
<br><br>
    <input type="text" name="displayName" class= "form-control sumber" placeholder="Username">
    <input type="email" name="username" class= "form-control sumber" placeholder="Email">
    <input type="password" name="password" class= "form-control sumber" placeholder="Password">
    <button type="submit" name="button" class= "btn btn-primary derlan">Submit</button>
  </form>
</section>
<%- include("partials/footer") -%>
